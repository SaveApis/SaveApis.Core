{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "SsoClient",
      "url": "https://sso.saveapis.de/graphql/",
      "dependencyInjection": true,
      "namespace": "SaveApis.SSO.Client",
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}